<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Home Directory">
        <Item>
          <IdeaActionID>FileChooser.GotoHome</IdeaActionID>
          <ShortCut>'Ctrl+1'</ShortCut>
          <Description>Home Directory</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Directory">
        <Item>
          <IdeaActionID>FileChooser.GotoProject</IdeaActionID>
          <ShortCut>'Ctrl+2'</ShortCut>
          <Description>Project Directory</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Shift+S' or 'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Execution Point">
        <Item>
          <IdeaActionID>ShowExecutionPoint</IdeaActionID>
          <ShortCut>'Alt+F10'</ShortCut>
          <Description>Show Execution Point</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Over">
        <Item>
          <IdeaActionID>StepOver</IdeaActionID>
          <ShortCut>'F8'</ShortCut>
          <Description>Step Over</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+2' or 'Ctrl+NumPad-2'</ShortCut>
          <Description>Terminal</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Update Project...">
        <Item>
          <IdeaActionID>Vcs.UpdateProject</IdeaActionID>
          <ShortCut>'Ctrl+T'</ShortCut>
          <Description>Update Project...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+5'</ShortCut>
          <Description>Version Control</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>